{
  "name": "node-module",
  "version": "2.0.0",
  "private": false,
  "author": "Ivo von Putzer Reibegg <ivo.putzer@gmail.com> (https://github.com/ivoputzer)",
  "description": "node-module is a boilerplate application for easier nodejs-module setup and maintenance",
  "homepage": "https://github.com/ivoputzer/node-module",
  "keywords": [
    "node",
    "module",
    "boilerplate",
    "maintenance",
    "config",
    "setup",
    "travis",
    "github"
  ],
  "contributors": [
    "Ivo von Putzer Reibegg <ivo.putzer@gmail.com> (https://github.com/ivoputzer)"
  ],
  "scripts": {
    "bin": "bin/node-module",
    "postinstall": "npm prune",
    "test": "mocha --recursive --full-trace --check-leaks",
    "version:auto": "bin/version/auto",
    "verify:major": "git reset head~2 -- test; npm test -- $(find test/module -type f -name \\*.js)",
    "verify:minor": "git reset head~2 -- test; npm test -- $(find test -type f -name \\*.js -not -path test/module/\\*)"
  },
  "bin": {
    "node-module" : "bin/node-module"
  },
  "main" : "index.js",
  "engineStrict": true,
  "engines": {
    "node": "^6.0.0",
    "iojs": "^3.0.0",
    "npm": "^3.0.0"
  },
  "config": {
    "analytics": false
  },
  "dependencies": {},
  "devDependencies": {
    "mocha": "^2.5.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivoputzer/node-module.git"
  },
  "bugs": {
    "url": "https://github.com/ivoputzer/node-module/issues"
  },
  "license": "license.md"
}
